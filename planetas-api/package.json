{
  "name": "planeta-api",
  "version": "1.0.0",
  "description": "Game Star Wars",
  "main": "src/loader.js",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon -L src/loader.js --exec babel-node",
    "prd": "babel-node ./src/loader.js",
    "start": "npm run prd",
    "mocha": "./node_modules/.bin/mocha --timeout 40000 tests/**/unit.*.js --require babel-register",
    "test": "set NODE_ENV=test && npm run mocha",
    "test:coverage": "./node_modules/.bin/nyc npm mocha"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ceciliano/montinho.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Rafael Braga <rafaelp.ceciliano@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "should": "^13.2.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "config": "^1.31.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "http-status": "^1.2.0",
    "ioredis": "^3.2.2",
    "mongoose": "^5.2.5",
    "redis": "^2.8.0",
    "request": "^2.87.0"
  }
}
